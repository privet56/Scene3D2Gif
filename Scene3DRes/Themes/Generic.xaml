<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Scene3DRes">

    <BitmapImage x:Key="ImageSourceGrid" UriSource="pack://application:,,,/Scene3DRes;component/res/grid.png" />
    
    <SolidColorBrush x:Key="buttonBackgroundColorBrush" Color="Beige" />
    <Color x:Key="buttonBackgroundColor">Beige</Color>

    <Style x:Key="Scene2DToggleButtonStyle" TargetType="{x:Type ToggleButton}">
        <Setter Property="Background" Value="{StaticResource buttonBackgroundColorBrush}"/>
    </Style>

</ResourceDictionary>
